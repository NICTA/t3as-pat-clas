<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
      version="3.0">

    <servlet>
        <servlet-name>CPCLookupServlet</servlet-name>
        <servlet-class>org.t3as.patClas.lookup.web.CPCLookupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CPCLookupServlet</servlet-name>
        <url-pattern>/v1.0/CPC/*</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>IPCLookupServlet</servlet-name>
        <servlet-class>org.t3as.patClas.lookup.web.IPCLookupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IPCLookupServlet</servlet-name>
        <url-pattern>/v1.0/IPC/*</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>USPCLookupServlet</servlet-name>
        <servlet-class>org.t3as.patClas.lookup.web.USPCLookupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>USPCLookupServlet</servlet-name>
        <url-pattern>/v1.0/USPC/*</url-pattern>
    </servlet-mapping>
    
</web-app>
